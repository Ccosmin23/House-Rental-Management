            <Label layoutX="197.0" layoutY="123.0" text="Sign in" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="tf_username" layoutX="138.0" layoutY="176.0" prefHeight="26.0" prefWidth="174.0" promptText="Username" />
            <PasswordField fx:id="tf_password" layoutX="137.0" layoutY="229.0" prefHeight="26.0" prefWidth="174.0" promptText="Password" />
